    "function EMQ(tray,NE,RE,ite)\n",
    "    E=tray[1:end,2]\n",
    "    n=size(unique(E),1) \n",
    "    N=zeros(n,n)\n",
    "    R=zeros(n)  \n",
    "    EN=zeros(n,n)\n",
    "    ER=zeros(n)\n",
    "    Q=zeros(n,n,ite)\n",
    "    for k in 1:ite\n",
    "        tra=MJPBridgeBisection(tray,Q[:,:,k])\n",
    "        m=length(tra[:,1])\n",
    "        N=cuenta(tra)[2]\n",
    "        R=cuenta(tra)[1]\n",
    "        for i in 1:n\n",
    "            for j in 1:n\n",
    "                if i!=j\n",
    "                    EN[i,j]=mean(N[i,j])\n",
    "                    ER[i]=mean(R[i])\n",
    "                    Q[i,j,k]=EN[i,j]/ER[i]\n",
    "                    Q[i,j,1]=mean(NE[i,j])/mean(RE[i])\n",
    "                end\n",
    "                \n",
    "            end\n",
    "            Q[i,i,k]=-sum(Q[:,:,k],2)[i]\n",
    "            Q[i,i,1]=-sum(Q[:,:,1],2)[i]\n",
    "        end\n",
    "\n",
    "    end\n",
    "    return (Q)\n",
    "end"
   ]
